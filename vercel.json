{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": { "runtime": "vercel-php@0.7.2" }
  },
  "routes": [
    {
      "src": "/build/assets/(.*)",
      "dest": "/public/build/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "X-Vercel-Route": "build-assets"
      }
    },
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "X-Vercel-Route": "build"
      }
    },
    {
      "src": "/(css|js|images|fonts|storage)/(.*)",
      "dest": "/public/$1/$2",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "X-Vercel-Route": "static"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php",
      "headers": {
        "Cache-Control": "no-store, no-cache, must-revalidate",
        "X-Vercel-Route": "php"
      }
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_URL": "https://another-iota.vercel.app",
    "APP_KEY": "base64:P1ntrpEpvXN+mym874KIj0fa/labvvLyHZyzDWXPsUQ=",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "database"
  }
}
